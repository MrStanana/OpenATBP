{
  "name": "api",
  "version": "0.0.1",
  "description": "Server to handle HTTP requests sent by the ATBP client",
  "main": "dist/server/main.js",
  "author": "OpenATBP Contributors",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node --trace-warnings --enable-source-maps ./dist/server/main.js",
    "build": "babel --config-file ./babel.config.js --extensions '.js,.jsx,.ts,.tsx' ./src --out-dir ./dist/server --copy-files --source-maps"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "ejs": "3.1.9",
    "express": "4.18.3",
    "mongodb": "6.5.0",
    "pino": "8.19.0",
    "pino-pretty": "10.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-typescript": "7.23.3",
    "@types/express": "4.17.21",
    "@types/node": "20.11.28",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-plugin-import": "2.29.1",
    "typescript": "5.4.2"
  }
}
